  "fileName": "ZCMonthlyIncomeOracle.sol",
  "contractName": "ZCMonthlyIncomeOracle",
  "source": "pragma solidity ^0.5.15;\n\nimport \"@openzeppelin/contracts-ethereum-package/contracts/ownership/Ownable.sol\";\n\ncontract ZCMonthlyIncomeOracle is Ownable {\n\n    struct IncomeData {\n        uint date;\n        uint amount;\n    }\n\n    mapping (address => IncomeData) public monthlyIncome;\n\n    event MonthlyIncomeUpdated(address _address, uint _amount);\n\n    function setMonthlyIncome(address _address, uint _amount) public onlyOwner {\n        monthlyIncome[_address] = IncomeData(now, _amount);\n        emit MonthlyIncomeUpdated(_address, _amount);\n    }\n\n    function getMonthlyIncome(address _address) public view returns (uint income) {\n        return monthlyIncome[_address].amount;\n    }\n\n    function getLastUpdate(address _address) public view returns (uint lastUpdate) {\n        return monthlyIncome[_address].date;\n    }\n}\n",
